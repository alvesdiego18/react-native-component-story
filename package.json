    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react": "^18.1.0",
    "react-native": "^0.70.15",
    "react-test-renderer": "^18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
